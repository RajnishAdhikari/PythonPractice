   "metadata": {},
   "outputs": [],
   "source": [
    "# we use yield in place of return in case of generator \n",
    "def Myrange(start, stop, step):\n",
    "    while start < stop:\n",
    "        # return start\n",
    "        yield start\n",
    "        start = start + step"
   ]
  },
